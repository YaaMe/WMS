<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>Cargoinservlet</servlet-name>
    <servlet-class>servlet.Cargoinservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Cargoutservlet</servlet-name>
    <servlet-class>servlet.Cargoutservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Cargomovservlet</servlet-name>
    <servlet-class>servlet.Cargomovservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Cargoreadyservlet</servlet-name>
    <servlet-class>servlet.Cargoreadyservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Loginservlet</servlet-name>
    <servlet-class>servlet.Loginservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Searchcargoservlet</servlet-name>
    <servlet-class>servlet.Searchcargoservlet</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>Cargoinservlet</servlet-name>
    <url-pattern>/cargoin.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Cargoutservlet</servlet-name>
    <url-pattern>/cargout.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Cargomovservlet</servlet-name>
    <url-pattern>/cargomov.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Cargoreadyservlet</servlet-name>
    <url-pattern>/cargoready.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Loginservlet</servlet-name>
    <url-pattern>/login.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Searchcargoservlet</servlet-name>
    <url-pattern>/searchcargo.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
